# This file is managed by Puppet - all changes will be overwritten.

server {
    server_name             files.<%= @baseHost %>;
    listen                  80;
    root                    /home/fileapi/project/file-system;
    client_max_body_size    20k;
    server_tokens           off;
    access_log              off;
    error_log               /home/fileapi/logs/files-error.log error;

    if ($request_method != "GET" ) {
        return 405;
    }

    rewrite ^(.*)$ $1 break;
}
